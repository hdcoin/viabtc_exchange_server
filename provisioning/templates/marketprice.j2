[Unit]
Description=Market price
After=network.target alertcenter.service redis.service

[Service]
Type=forking
SyslogIdentifier=marketprice
ExecStart={{root_dir}}/marketprice/marketprice.exe {{root_dir}}/marketprice/config.json

Restart=always
PrivateTmp=true
TimeoutStopSec=60s
TimeoutStartSec=10s
StartLimitInterval=120s
StartLimitBurst=5

[Install]
WantedBy=multi-user.target
